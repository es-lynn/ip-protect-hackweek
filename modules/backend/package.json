{
  "name": "@ip-protect/backend",
  "version": "0.0.0",
  "description": "",
  "author": "es-lynn",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn prisma generate",
    "build": "nest build",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "db:start": "docker start -a ip-protect-db",
    "db:setup": "docker create --name ip-protect-db -p 5432:5432 -e POSTGRES_USER=dev -e POSTGRES_PASSWORD=password -e POSTGRES_DB=ip-protect postgres:13.6",
    "db:seed": "ts-node ./prisma/scripts/seed.ts",
    "db:reset": "prisma migrate reset",
    "compile": "tsc --noEmit && eslint . --ext .ts,.tsx --max-warnings=0",
    "compile:fix": "tsc --noEmit && eslint . --ext .ts,.tsx --fix --plugin unused-imports --fix --rule 'unused-imports/no-unused-imports-ts: warn'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@prisma/client": "4.7.0",
    "aws-sdk": "^2.1267.0",
    "axios": "^1.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "faker": "^5.5.3",
    "jose": "^4.11.1",
    "prisma": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-method-cache": "^3.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "globalSetup": "../jest/scripts/jestGlobalSetup.ts",
    "globalTeardown": "../jest/scripts/jestGlobalTeardown.ts",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
